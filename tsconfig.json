{
   "include": ["src"],
   "exclude": ["example", "dist", "node_modules"],
   "compilerOptions": {
      "jsx": "react",
      "declarationMap": true,
      "skipLibCheck": true,
      "target": "es5",
      "module": "commonjs",
      "lib": ["es2017", "es7", "es6", "dom"],
      "declaration": true,
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true
   }
}
